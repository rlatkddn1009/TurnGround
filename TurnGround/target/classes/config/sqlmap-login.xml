<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC 
	"-//mybatis.org//DTD Mapper 3.0//EN" 
	"http://mybatis.org/dtd/mybatis-3-mapper.dtd">
	
<mapper namespace="com.tug.dao.LoginDAO">

	<insert id="userJoin" parameterType="com.tug.vo.UserLoginVO">
		insert into userlogin (id, userid, userpw, username)
			values(#{id}, #{userid}, #{userpw}, #{username})
	</insert>
	
</mapper>